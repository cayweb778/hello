#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/vite@2.9.15/node_modules/vite/bin/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/vite@2.9.15/node_modules/vite/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/vite@2.9.15/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/node_modules:/home/jesse/IdeaProjects/node_modules:/home/jesse/node_modules:/home/node_modules:/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/vite/bin/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/vite@2.9.15/node_modules/vite/bin/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/vite@2.9.15/node_modules/vite/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/vite@2.9.15/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/.pnpm/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/node_modules:/home/jesse/IdeaProjects/node_modules:/home/jesse/node_modules:/home/node_modules:/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/vite/bin/node_modules:/home/jesse/IdeaProjects/boozsoft-nc1104/webs/packages/vite-plugin-vue-inspector/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
